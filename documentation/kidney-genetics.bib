	keywords = {Humans, Nephrology, Renal Insufficiency, Chronic, genome-wide association studies, genetic testing, genetic kidney disease, Congresses as Topic, monogenic, polygenic, single-nucleotide polymorphism},
	pages = {1126--1141},
